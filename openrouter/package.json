    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.4",
    "@cloudflare/workers-types": "^4.20251014.0",
    "@decocms/mcps-shared": "1.0.0",
    "@mastra/core": "^0.24.0",
    "@modelcontextprotocol/sdk": "^1.21.0",
    "@types/mime-db": "^1.43.6",
    "deco-cli": "^0.26.0",
    "typescript": "^5.7.2",
    "vite": "7.2.0",
    "wrangler": "^4.28.0"
